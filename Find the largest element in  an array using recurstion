class Main{
    public static void main(String args[])
    {
        int arr[]={34,65,23,76,87,32,122};
        System.out.println(largest(arr));
    }
    static int largest(int arr[])
    {
        int max=arr[0];
        for(int i=1;i<arr.length;i++)
        {
            if(arr[i]>max)
            {
                max=arr[i];
            }
        }
        return max;
    }
}
